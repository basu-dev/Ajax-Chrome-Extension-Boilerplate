{
    "manifest_version":2,
    "name":"Ajax_Boilerplate",
    "description":"boilerplate to make ajax call",
    "version":"1.0.0",
    "background": {
        "scripts": ["jquery.min.js","background.js"],
        "persistent": true
      },
      "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["jquery.min.js","content.js"]
        }
    ]
     
   

}